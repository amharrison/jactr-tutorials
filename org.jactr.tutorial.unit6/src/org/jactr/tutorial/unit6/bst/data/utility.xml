<instrument window="0.1">   <!-- how often to poll -->
 <group id="Expected Utilities">  <!-- everything in a group is rendered on the same graph -->
  <model pattern=".*">  <!-- what model to probe -->
   <production pattern="(decide|force)-(over|under)"> <!-- chunks of this type are to be probed -->
     <probe pattern="ExpectedUtility" poll="true"/> <!-- what parameter(s) -->
   </production>
  </model>
 </group>
</instrument>